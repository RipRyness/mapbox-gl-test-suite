{
  "version": 8,
  "constants": {
    "@constant": {
      "type": "dash-array",
      "value": [
        5,
        5
      ]
    },
    "@small": {
      "type": "number",
      "value": 2.5
    },
    "@large": {
      "type": "number",
      "value": 5
    }
  },
  "sources": {
    "mapbox": {
      "type": "vector",
      "maxzoom": 14,
      "tiles": [
        "local://tiles/{z}-{x}-{y}.vector.pbf"
      ]
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "white"
      }
    },
    {
      "id": "road",
      "type": "line",
      "source": "mapbox",
      "source-layer": "road",
      "paint": {
        "line-width": 2,
        "line-color": "#000"
      },
      "paint.literal": {
        "line-dasharray": [
          2.5,
          2.5
        ]
      },
      "paint.constant": {
        "line-dasharray": "@constant"
      },
      "paint.array-constant": {
        "line-dasharray": [
          "@small",
          "@large"
        ]
      },
      "paint.function": {
        "line-dasharray": {
          "domain": [
            16,
            17
          ],
          "range": [
            [
              7.5,
              7.5
            ],
            [
              7.5,
              7.5
            ],
            [
              10,
              10
            ]
          ],
          "type": "interval"
        }
      }
    }
  ]
}
