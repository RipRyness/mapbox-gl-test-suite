{
  "version": 5,
  "sources": {
    "mapbox": {
      "type": "vector",
      "maxzoom": 14,
      "tiles": ["local://tiles/{z}-{x}-{y}.vector.pbf"]
    }
  },
  "constants": {
    "@constant": [10, 10],
    "@small": 5,
    "@large": 10
  },
  "layers": [{
    "id": "background",
    "type": "background",
    "style": {
      "background-color": "white"
    }
  }, {
    "id": "road",
    "source": "mapbox",
    "source-layer": "road",
    "type": "line",
    "style": {
      "line-width": 2,
      "line-color": "#000"
    },
    "style.literal": {
      "line-dasharray": [5, 5]
    },
    "style.constant": {
      "line-dasharray": "@constant"
    },
    "style.array-constant": {
      "line-dasharray": ["@small", "@large"]
    },
    "style.function": {
      "line-dasharray": [{
        "stops": [[16, 15], [17, 20]]
      }, {
        "stops": [[16, 15], [17, 20]]
      }]
    }
  }]
}
