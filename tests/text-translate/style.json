{
  "version": 8,
  "constants": {
    "@constant": {
      "type": "translate-array",
      "value": [
        10,
        10
      ]
    }
  },
  "sources": {
    "mapbox": {
      "type": "vector",
      "maxzoom": 14,
      "tiles": [
        "local://tiles/{z}-{x}-{y}.vector.pbf"
      ]
    }
  },
  "glyphs": "local://glyphs/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "white"
      }
    },
    {
      "id": "text",
      "type": "symbol",
      "source": "mapbox",
      "source-layer": "poi_label",
      "filter": [
        "==",
        "maki",
        "restaurant"
      ],
      "layout": {
        "symbol-placement": "point",
        "text-allow-overlap": true,
        "text-ignore-placement": true,
        "text-field": "Test",
        "text-font": [
          "Open Sans Semibold",
          "Arial Unicode MS Bold"
        ]
      },
      "paint.literal": {
        "text-translate": [
          5,
          5
        ]
      },
      "paint.constant": {
        "text-translate": "@constant"
      },
      "paint.function": {
        "text-translate": {
          "domain": [
            16,
            17
          ],
          "range": [
            [
              15,
              15
            ],
            [
              20,
              20
            ]
          ],
          "type": "exponential"
        }
      }
    }
  ]
}
