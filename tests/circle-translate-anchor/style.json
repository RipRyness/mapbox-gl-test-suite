{
  "version": 8,
  "constants": {},
  "sources": {
    "mapbox": {
      "type": "vector",
      "maxzoom": 14,
      "tiles": [
        "local://tiles/{z}-{x}-{y}.vector.pbf"
      ]
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "white"
      }
    },
    {
      "id": "road",
      "type": "circle",
      "source": "mapbox",
      "source-layer": "road",
      "paint": {
        "circle-color": "black",
        "circle-translate": [
          10,
          10
        ]
      },
      "paint.map": {
        "circle-translate-anchor": "map"
      },
      "paint.viewport": {
        "circle-translate-anchor": "viewport"
      }
    }
  ]
}
