{
  "version": 5,
  "sources": {
    "mapbox": {
      "type": "vector",
      "maxzoom": 14,
      "tiles": ["http://localhost:2900/tiles/{z}-{x}-{y}.vector.pbf"]
    }
  },
  "sprite": "http://localhost:2900/sprites/sprite",
  "constants": {
    "@constant": "0.4"
  },
  "layers": [{
    "id": "background",
    "type": "background",
    "style": {
      "background-opacity": 0.5
    },
    "style.color": {
      "background-color": "red"
    },
    "style.image": {
      "background-image": "wetland-17"
    }
  }]
}
